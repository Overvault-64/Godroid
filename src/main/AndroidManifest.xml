<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="com.android.alarm.permission.SET_ALARM"/>
    <uses-permission android:name="android.permission.SCHEDULE_EXACT_ALARM"/>

    <application>
        <meta-data
            android:name="org.godotengine.plugin.v1.GodroidReview"
            android:value="org.overvault.godroid.Review" />

        <meta-data
            android:name="org.godotengine.plugin.v1.GodroidToast"
            android:value="org.overvault.godroid.Toast" />

        <meta-data
            android:name="org.godotengine.plugin.v1.GodroidShare"
            android:value="org.overvault.godroid.Share" />

        <meta-data
            android:name="org.godotengine.plugin.v1.GodroidNotifications"
            android:value="org.overvault.godroid.Notifications" />

        <receiver android:name="org.overvault.godroid.NotificationsReceiver" android:process=":remote" />

    </application>

</manifest>

